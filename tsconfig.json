{
    "include": [
        "src"
    ],
    "compilerOptions": {
        "target": "es2015",
        "module": "system",
        "outFile": "bundle.js",
        "pretty": true, 
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitUseStrict": true,
        "noUnusedLocals": true
    }
}